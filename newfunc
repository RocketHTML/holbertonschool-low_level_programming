#!/bin/bash

if [ "$#" -ne 2 ]; then
	echo "Usage: '<filename>' '<prototype>'"
	exit 1
fi

if [ ! -d $current_folder ]; then
mkdir $current_folder
cp $h_file $current_folder
cp README.md $current_folder
fi

set -f
./addheader $current_folder "$2;"
./addheader . "$2;"
./create "$2" > $current_folder/$1
